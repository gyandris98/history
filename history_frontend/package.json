{
  "name": "@elastic/next-eui-starter",
  "private": true,
  "description": "Start building protoypes quickly with the Next.js EUI Starter",
  "version": "1.0.0",
  "author": "Rory Hunter <rory@elastic.co>",
  "keywords": [
    "next",
    "kibana",
    "eui",
    "elastic",
    "typescript"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "yarn lint && rm -f public/themes/*.min.css && next build",
    "build-docs": "bash scripts/update-docs.sh",
    "dev": "env NODE_TLS_REJECT_UNAUTHORIZED='0' next",
    "lint": "tsc && eslint --cache 'src/**/*.{ts,tsx,js}' --max-warnings 0",
    "start": "next start",
    "test-docs": "bash scripts/test-docs.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elastic/next-eui-starter"
  },
  "bugs": {
    "url": "https://github.com/elastic/next-eui-starter/issues"
  },
  "dependencies": {
    "@editorjs/editorjs": "^2.19.1",
    "@editorjs/embed": "^2.4.0",
    "@editorjs/header": "^2.6.1",
    "@editorjs/image": "^2.6.0",
    "@editorjs/list": "^1.6.2",
    "@editorjs/paragraph": "^2.8.0",
    "@editorjs/quote": "^2.4.0",
    "@elastic/datemath": "^5.0.3",
    "@elastic/eui": "^30.5.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@next/bundle-analyzer": "^10.0.3",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.21.1",
    "cookies": "^0.8.0",
    "dayjs": "^1.10.4",
    "glob": "^7.1.6",
    "js-cookie": "^2.2.1",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "next": "^10.0.3",
    "react": "^17.0.1",
    "react-autosize-textarea": "^7.1.0",
    "react-dom": "^17.0.1",
    "react-editor-js": "^1.9.0",
    "react-hook-form": "^6.15.1",
    "react-images-upload": "^1.2.8",
    "react-query": "^3.9.7",
    "react-query-auth": "^0.2.0",
    "sass": "^1.29.0",
